{
  "name": "tbread-art",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "cms",
    "gallery"
  ],
  "scripts": {
    "build": "npm run build --workspace cms && npm run build --workspace gallery",
    "start": "npm-run-all --parallel start:cms start:gallery",
    "start:cms": "npm run start --workspace cms",
    "start:gallery": "npm run dev --workspace gallery"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
